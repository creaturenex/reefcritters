          <div class="card">
            <div class="card-image">
              <figure class="image is-4by3">
                <%= image_tag "001.jpeg" %>
              </figure>
            </div>
            <div class="card-content">
              <div class="media">
                <div class="media-left">
                  <figure class="image is-48x48">
                    <i class="fa-solid fa-alien-8bit"></i>
                  </figure>
                </div>
                <div class="media-content">
                  <p class="title is-3">Frankie's Pink Rim</p>
                </div>
              </div>
              <div class="content ">
                <div class="level">
                  <div class="level-item">
                    <div class=" tags has-addons">
                      <span class="tag">Flow</span>
                      <span class="tag is-primary">High</span>
                    </div>
                  </div>
                  <div class="level-item">
                    <div class="tags has-addons">
                      <span class="tag">Light</span>
                      <span class="tag is-primary">High</span>
                    </div>
